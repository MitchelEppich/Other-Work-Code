        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Brain
alive = _health&gt;0;
// Controls 
if (!stunned &amp;&amp; alive) scr_soldier_controls();//scr_player_controls();
// Collision ////////////////////////////
scr_player_collision();
// Gravity //////////////////////////////
grounded = scr_player_grounded();
// Scale and animation //////////////////
scr_player_keep_stats();

if (grounded) 
{ 
    if (!_stealth)
    {
        if (hsp == 0 &amp;&amp; vsp == 0) 
        {
            _state = scr_player_stand;
            if (_follow) _state = scr_player_stand_follow;
            if (_with) _state = scr_player_stand_with;
        }
        
        if (hsp != 0) 
        {
            _state = scr_player_walk;
            if (_follow) _state = scr_player_walk_follow;
            if (_with) _state = scr_player_walk_with;
        }
        
        if (abs(hsp) &gt; grnd_max/2) 
        {
            _state = scr_player_run;
            if (_follow) _state = scr_player_run_follow;
            if (_with) _state = scr_player_run_with;
        }
        
    } else {
        if (hsp == 0 &amp;&amp; vsp == 0) _state = scr_player_stealth_stand;
        if (hsp != 0) _state = scr_player_stealth_walk; 
    }
    if (attacking) _state = scr_player_attack;
    if (rolling) _state = scr_player_roll;
    if (!alive) _state = scr_player_death;
} else if (scaling) { 
    _state = scr_player_scale;
} else {
    _state = scr_player_jump;
}

if (stunned) _state = scr_player_stun;

script_execute(_state);
</string>
