    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Commence command

unknown = false;

if (command != "") {

    // Normalize command text
    command = string_lower(command);

    var protocol = 0;
    var order = 0;
    var type = 0;
    var amount = string(1);
    var enabled = string(0);

    // Analyze command
    protocol = string_char_at(command, 0);
    
    command = string_delete(command, 1, 1); // remove protocol from command
    
    order = scr_substring(command, 0, " ");
    type = scr_substring(command, 1, " ");
    if (scr_substring(command, 2, " ") != "") amount = scr_substring(command, 2, " ");
    if (scr_substring(command, 3, " ") != "") enabled = scr_substring(command, 3, " ");
    
    var command_report = "CMD :: ";
    
    if (protocol == "/") {
        switch (order) {
            // Spawn an instance
            case "spawn":
            scr_dev_cmd_spawn(type, amount, enabled);
            break;
            case "destroy":
            scr_dev_cmd_destroy(type);
            break;
            case "give":
            scr_dev_cmd_give(type, amount);
            break;
            case "deploy":
            scr_dev_cmd_destroy(type);
            break;
            case "whatis":
            type = scr_dev_cmd_whatis(type);
            break;
            case "lookat":
            type = scr_dev_cmd_lookat(type);
            break;
            case "start":
            repeat(100) {
                var inst = instance_create (irandom(room_width), 50, obj_human);
                inst.team = choose("foe", "ally");
            }
            repeat(10) {
                instance_create (irandom(room_width), 50, obj_package);
            }
            break;
            case "":
            unknown = true;
            break;
        }
        command_report += "-" + string(amount) + "- INSTANCE -" + string(order) + "- OF TYPE -" + string(type) + "-";
    } else unknown = true;
    
    if (unknown) command_report = "CMD :: ERROR -unknown command-"; 
    
    show_debug_message(command_report);
    
    // Clear command
    command = "";
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
