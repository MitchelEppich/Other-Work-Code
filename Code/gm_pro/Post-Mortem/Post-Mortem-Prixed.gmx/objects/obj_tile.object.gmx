          <argument>
            <kind>1</kind>
            <string>/// Draw sprite

if (growth == -1) growth = trim;

// Draw dirt
draw_sprite_ext(sprite, image_index, x, y, image_xscale, image_yscale, image_angle, c_white, 1);

if (trim != -1) {

    // Draw trim
    if (right) {
        draw_sprite_ext(trim, 0, x + sprite_width, y, image_xscale, image_yscale, -90, c_white, 1);
    }
    
    // Draw trim
    if (left) {
        draw_sprite_ext(trim, 0, x, y + sprite_height, image_xscale, image_yscale, 90, c_white, 1);
    }
    
    // Draw trim
    if (bottom) {
        draw_sprite_ext(trim, 0, x + sprite_width, y + sprite_height, image_xscale, image_yscale, 180, c_white, 1);
    }

}

if (growth != -1) {
    // Draw grass
    //show_debug_message(string(scr_check_covered(x, y)));
    if(top) {
        if ((scr_check_covered(x, y)) &amp;&amp; trim != -1) 
            draw_sprite_ext(trim, 0, x, y, image_xscale, image_yscale, 0, c_white, 1);
        else draw_sprite_ext(growth, 0, x, y, image_xscale, image_yscale, 0, c_white, 1);
    } else {
        var inst = instance_place(x, y-1, obj_tile);
        
        if (inst == noone) exit;
        
        if ((scr_check_covered(x, y) || inst.enable_growth == 2) &amp;&amp; trim != -1) 
            draw_sprite_ext(trim, 0, x, y, image_xscale, image_yscale, 0, c_white, 1);
        else draw_sprite_ext(growth, 0, x, y, image_xscale, image_yscale, 0, c_white, 1);
    }
}

if (onFire){} 
