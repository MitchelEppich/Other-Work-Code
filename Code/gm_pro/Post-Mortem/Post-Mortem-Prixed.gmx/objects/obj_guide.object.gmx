            <kind>1</kind>
            <string>/// Update speech to be drawn

if characters&lt;message_length{ //if current character count is less than the amount in current message
    hold=global.key[? "interact"]; //hold is true or false if we hold 'Z' or not
    characters+=increase*(1+hold); //increase speed based on hold
    message_draw=string_copy(message_current,0,characters); //copy string to current character
}
else{ //if current character is more than the amount in the current message
    if global.key[? "interact"] { 
        if ds_queue_size(speech_q) != 0 { 
            message_current = ds_queue_head(speech_q);
            ds_queue_dequeue(speech_q);
            sprite = ds_queue_head(animation_q);
            ds_queue_dequeue(animation_q);
            message_length=string_length(message_current);
            characters=0;
            message_draw="";
        } else { 
            sprite = spr_guide_silent;
            message_draw = "";
        }
    }
}
</string>
