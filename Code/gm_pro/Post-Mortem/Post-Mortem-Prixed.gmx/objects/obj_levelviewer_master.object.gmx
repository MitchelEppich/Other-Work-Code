        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw map
draw_set_font(font_main);

draw_set_halign(fa_center);
draw_set_color(c_white);
draw_set_alpha(1);
draw_text_transformed(room_width/2, 100, "Level Selection", 2, 2, 0);

var colour = c_white;
var alpha = 0.5;

menu_items = 30;

// Create random seed to ensure same output all the time
random_set_seed(real("MitchelEppich"));

// For line connections
var last_xx = 50;
var last_yy = view_hview[0]/2;

for (var i = 0; i &lt; menu_items; i++) {
    // Get random location
    var modifier = sign(irandom(2) - 1);
    var xx = last_xx + 30;
    var yy = last_yy + max(20, irandom(40))*modifier;
    
    // Draw connecting lines
    if(menu_selection &gt;= i) { colour = c_red; alpha = 0.4; } else { colour = c_white; alpha = 0.5; }
    draw_set_color(colour);
    draw_set_alpha(alpha);
    if(i != 0) draw_line_width(xx, yy, last_xx, last_yy, 4);
    
    // Draw dots
    if(menu_selection == i) { colour = c_red; alpha = 1; } else { colour = c_white; alpha = 0.5; }
    draw_set_color(colour);
    draw_set_alpha(alpha);
    draw_circle(xx, yy, 5, 0);
    
    last_xx = xx;
    last_yy = yy;
}

draw_set_color(-1);
draw_set_alpha(1);

if (key_enter) {
    room_goto(rm_testing);
}
</string>
