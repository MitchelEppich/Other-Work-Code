var vspd = lengthdir_y (ylen, dir);

// If not grounded do not allow movement
if (!grounded) { 
    hspd = 0;
    vspd = 0;
}

// Apply current movement through impulse
physics_apply_local_impulse(x, y, hspd - phy_speed_x, vspd);
