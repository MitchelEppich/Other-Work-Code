            <kind>1</kind>
            <string>/// Place tile
var xx, yy;
if (image_xscale == 1) {
    xx = floor(mouse_x/16)*16;
    yy = floor(mouse_y/16)*16;
}

if (image_xscale == -1) {
    xx = ceil(mouse_x/16)*16;
    yy = floor(mouse_y/16)*16;
}

if (!instance_place(xx, yy, obj_instance)) {
    var there = instance_place(xx, yy, global.tile);
    var inst = instance_create(xx, yy, global.tile);
    
    if (there != noone) {
        with inst instance_destroy();
        exit;
    }
    inst.x = xx;
    inst.y = yy;
    inst.image_xscale = image_xscale;
}
</string>
